{
    "name": "test-network-org3",
    "version": "1.0.0",
    "client": {
        "organization": "Org3",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Org3": {
            "mspid": "Org3MSP",
            "peers": [
                "peer0.org3.example.com"
            ],
            "certificateAuthorities": [
                "ca.org3.example.com"
            ]
        }
    },
    "peers": {
        "peer0.org3.example.com": {
            "url": "grpcs://localhost:11051",
            "tlsCACerts": {
                "pem": ""
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.org3.example.com",
                "hostnameOverride": "peer0.org3.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.org3.example.com": {
            "url": "https://localhost:11054",
            "caName": "ca-org3",
            "tlsCACerts": {
                "pem": [""]
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
